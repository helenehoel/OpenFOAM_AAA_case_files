FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.375;

boundaryField
{
    walls
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    LI
    {
        type            zeroGradient;
    }
    aortaInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    RI
    {
        type            zeroGradient;
    }
}


